{
    "name": "bottle",
    "builds": {
        "gallery-bt-pushpop": {
            "config": {
                "requires": [
                    "base-build",
                    "widget-parent",
                    "gallery-bt-container"
                ]
            },
            "jsfiles": [
                "pushpop/pushpop.js"
            ]
        },
        "gallery-bt-overlay": {
            "config": {
                "requires": [
                    "widget-position",
                    "widget-stack",
                    "gallery-bt-pushpop"
                ]
            },
            "jsfiles": [
                "overlay/overlay.js"
            ]
        },
        "gallery-bt-viewer": {
            "config": {
                "requires": [
                    "gallery-bt-syncscroll",
                    "gallery-zui-rascroll",
                    "gallery-zui-attribute",
                    "gallery-zui-scrollhelper"
                ]
            },
            "jsfiles": [
                "viewer/viewer.js"
            ]
        },
        "gallery-bt-slidetab": {
            "config": {
                "requires": [
                    "gallery-bt-syncscroll",
                    "widget-stdmod",
                    "gallery-zui-rascroll",
                    "gallery-zui-scrollsnapper"
                ]
            },
            "jsfiles": [
                "slidetab/slidetab.js"
            ]
        },
        "gallery-bt-container": {
            "config": {
                "requires": [
                    "scrollview",
                    "widget-child",
                    "json-parse",
                    "gallery-zui-attribute",
                    "gallery-zui-rascroll",
                    "gallery-zui-scrollhelper",
                    "gallery-bt-device"
                ]
            },
            "jsfiles": [
                "container/container.js"
            ]
        },
        "gallery-bottle": {
            "config": {
                "skinnable": "true",
                "requires": [
                    "gallery-bt-shortcut",
                    "gallery-bt-overlay",
                    "gallery-bt-photogrid",
                    "gallery-bt-slidetab",
                    "gallery-bt-carousel",
                    "gallery-bt-loader",
                    "gallery-bt-viewer"
                ]
            },
            "skinnable": "true",
            "jsfiles": [
                "bottle/bottle.js"
            ]
        },
        "gallery-bt-loader": {
            "config": {
                "requires": [
                    "gallery-bt-syncscroll",
                    "io-base",
                    "json-parse"
                ]
            },
            "jsfiles": [
                "loader/loader.js"
            ]
        },
        "gallery-bt-photogrid": {
            "config": {
                "requires": [
                    "gallery-bt-syncscroll"
                ]
            },
            "jsfiles": [
                "photogrid/photogrid.js"
            ]
        },
        "gallery-bt-syncscroll": {
            "config": {
                "requires": [
                    "gallery-bt-page"
                ]
            },
            "jsfiles": [
                "syncscroll/syncscroll.js"
            ]
        },
        "gallery-bt-device": {
            "config": {
                "requires": [
                    "node-screen"
                ]
            },
            "jsfiles": [
                "device/device.js"
            ]
        },
        "gallery-bt-page": {
            "config": {
                "requires": [
                    "widget-position",
                    "widget-stack",
                    "gallery-bt-pushpop"
                ]
            },
            "jsfiles": [
                "page/page.js"
            ]
        },
        "gallery-bt-carousel": {
            "config": {
                "requires": [
                    "gallery-bt-syncscroll",
                    "gallery-zui-rascroll",
                    "gallery-zui-scrollsnapper",
                    "gallery-zui-attribute",
                    "gallery-zui-scrollhelper"
                ]
            },
            "jsfiles": [
                "carousel/carousel.js"
            ]
        },
        "gallery-bt-shortcut": {
            "config": {
                "requires": [
                    "gallery-bt-page"
                ]
            },
            "jsfiles": [
                "shortcut/shortcut.js"
            ]
        }
    }
}
